/* SCAN-IN
CJ2C_Func 3.000000
C2CJ_Func 11.000000
Func 11.000000
get from CJ struct: s -> 0.000000
get from C struct: s -> 0.000000
Exception
 */

@C
public func C2CJ_Func(a: Float64): Float64 {
    println("C2CJ_Func " + a.toString())
    var b: Float64 = Func(a)
    return b
}

func Func(a: Float64): Float64 {
    println("Func " + a.toString())
    throw Exception()
    return a
}

func CJ2C_Func(a: Float64): Float64 {
    println("CJ2C_Func " + a.toString())
    var b = unsafe { C_Func(a) }
    return b
}

foreign func C_Func(a: Float64): Float64

main(): Int64 {
    try {
        var bbb: Float64 = CJ2C_Func(3.0)
        println(bbb.toString())
        if (bbb == 11.0) {
            return 1
        }
    } catch (e: Exception) {
        println(e.toString())
    }
    return 0
}
