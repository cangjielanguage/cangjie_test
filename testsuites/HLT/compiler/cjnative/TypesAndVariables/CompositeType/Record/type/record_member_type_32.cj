// LEVEL: 1
// EXEC: %compiler %cmp_opt %f -o %output
// RUN-EXEC: %run %run_opt %output %run_args

class Foo {
    public let x: Int32 = 3
}

struct Test {
    public let a: Foo
    public init(x: Foo) {
        a = x
    }
}

let res = Test(Foo())
var foo = res.a
let res1 = foo.x

func run(): Int64 {
    if (res1 == 3) {
        return 0
    }
    return 1
}

main(): Int64 {
    return run()
}
