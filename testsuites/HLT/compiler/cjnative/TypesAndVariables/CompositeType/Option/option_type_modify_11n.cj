// LEVEL: 1
// EXEC-PIPE-1: %compiler %cmp_opt %f -o %output 2>&1 | compare %f



operator var opt: Option<String> = .Some<String>("option")  

func run(): Int64 {
    var str: String = match (opt) {
        case Some(x) => x
        case None => ""
    }
    if(str == "option"){
        return 0
    }
    return 1
}

main(): Int64 {
    return run()
}
