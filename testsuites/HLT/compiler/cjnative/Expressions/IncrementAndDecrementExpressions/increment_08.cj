// LEVEL: 1
// EXEC-PIPE-1: %compiler %cmp_opt %f -o %output 2>&1 | compare %f

main(): Int64 {
    return run()
}

open class Base{
    public var c:Int32 = 2
}

class COO <: Base{
    public var a: Int32 = 1
    public var b: Int32 = 2
    public operator func ++():Int32{ 
        this.a = this.a + 1
        return this.a
    }
}

func run(): Int64 {
    var coo: COO = COO()
    var res = coo++ 
    if (res != 2) { return 1}
    return 0
}
