// LEVEL: 0
// EXEC: %compiler %cmp_opt %f -o %output
// RUN-EXEC: %run %run_opt %output %run_args

main(): Int64 {
    var b = Boo()
    b.m = 1
    if (b.a != 2) {
        return 1
    }
    b.a = 3
    if (b.a != 3) {
        return 1
    }
    return 0
}

struct Boo {
    public var a: Int32 = 1
    public mut prop m: Int32 {
        get() {
            return a
        }
        set(input) {
            a = input + 1
        }
    }

    public prop n: Int32 {
        get() {
            return a
        }
    }
}
