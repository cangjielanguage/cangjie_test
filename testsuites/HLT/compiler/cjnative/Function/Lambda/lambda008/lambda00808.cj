extend<T> Array<T> {
    public func foooo(idx: Int64) {
        return -1
    }
}

main(): Int64 {
    let f = {
        x =>
        let arr = ["1", "2"]
        arr.foooo(0) - x
    }
    if (f is (Int64) -> Int64) {
        return 0
    }
    return 1
}
