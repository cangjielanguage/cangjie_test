// LEVEL: 0
// EXEC-PIPE-1: %compiler %cmp_opt %f -o %output 2>&1 | compare %f

main(): Int64 {
    var a = Coo()
    if (a.n != 1) {
        return 1
    }
    a.m = 10
    if (a.m != 2) {
        return 2
    }
    return 0
}

abstract class Boo {
    public mut prop m: Int32
    public prop n: Int32
}

extend Boo {
    public mut prop m: Int32 {
        get() {
            return 2
        }
        set(v) {}
    }
    public prop n: Int32 {
        get() {
            return 1
        }
    }
}

class Coo <: Boo {
    public mut prop m: Int32 {
        get() {
            return 2
        }
        set(v) {}
    }
    public prop n: Int32 {
        get() {
            return 1
        }
    }
}
