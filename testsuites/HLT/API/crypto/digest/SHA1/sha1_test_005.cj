import stdx.crypto.digest.*
import std.convert.*
import std.crypto.digest.*
import stdx.encoding.hex.*

main() {
    var str: String = "Weclome#MD5$to_Cangjie"
    var sha1Instance = SHA1()
    var sha1Length: Int64 = sha1Instance.size
    var sha1BlockSize: Int64 = sha1Instance.blockSize

    if (sha1Length != 20 || sha1BlockSize != 64) {
        return 1
    }
    return 0
}
