// LEVEL: 1
// EXEC: %compiler %import-cangjie-stdx %cmp_opt -o  %output %f
// RUN-EXEC: %run_stdx %output %run_args 

import stdx.crypto.digest.*
import std.convert.*
import std.crypto.digest.*
import stdx.encoding.hex.*

main() {
    var str: String = "Weclome#MD5$to_Cangjie"
    var sha224Instance = SHA224()
    var sha224Length: Int64 = sha224Instance.size
    var sha224BlockSize: Int64 = sha224Instance.blockSize

    if (sha224Length != 28 || sha224BlockSize != 64) {
        return 1
    }
    return 0
}
