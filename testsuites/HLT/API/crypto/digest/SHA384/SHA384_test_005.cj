// LEVEL: 1
// EXEC: %compiler %import-cangjie-stdx %cmp_opt -o  %output %f
// RUN-EXEC: %run_stdx %output %run_args 

import stdx.crypto.digest.*
import std.convert.*
import std.crypto.digest.*
import stdx.encoding.hex.*

main() {
    var str: String = "Weclome#MD5$to_Cangjie"
    var sha384Instance = SHA384()
    var sha384Length: Int64 = sha384Instance.size
    var sha384BlockSize: Int64 = sha384Instance.blockSize

    if (sha384Length != 48 || sha384BlockSize != 128) {
        return 1
    }
    return 0
}
