// LEVEL: 1
// EXEC: %compiler %import-cangjie-stdx %cmp_opt -o  %output %f
// RUN-EXEC: %run_stdx %output %run_args 

import stdx.crypto.digest.*
import std.convert.*
import std.crypto.digest.*
import stdx.encoding.hex.*

main() {
    var str: String = "Weclome#MD5$to_Cangjie"
    var sha512Instance = SHA512()
    var sha512Length: Int64 = sha512Instance.size
    var sha512BlockSize: Int64 = sha512Instance.blockSize

    if (sha512Length != 64 || sha512BlockSize != 128) {
        return 1
    }
    return 0
}
