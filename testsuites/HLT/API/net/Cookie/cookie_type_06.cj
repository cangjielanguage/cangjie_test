// LEVEL: 1
// EXEC: %compiler %import-cangjie-stdx %cmp_opt %f -o %output
// RUN-EXEC: %run_stdx %output %run_args

import stdx.net.http.*
import std.time.*
import std.collection.*

var time1 = DateTime.of(year: 2022, month: January, dayOfMonth: 19)
var cookie = Cookie("0123456789", "0123456789", path: "/a1/", domain: ".examplenet.com", expires: time1, maxAge: 1,
    secure: true, httpOnly: true)
var cookie1 = Cookie("0123456789", "0123456789", path: "/a1/", domain: ".examplenet.com", expires: time1, maxAge: 1000,
    secure: true, httpOnly: true)

main(): Int64 {
    var a = cookie.toSetCookieString()
    var a1 = cookie1.toSetCookieString()
    if (a.contains("Max-Age=1") != true) {
        return 1
    }
    if (a1.contains("Max-Age=1000") != true) {
        return 2
    }
    return 0
}
