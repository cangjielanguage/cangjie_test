// LEVEL: 1
// EXEC: %compiler %import-cangjie-stdx %cmp_opt %f -o %output
// RUN-EXEC: %run_stdx %output %run_args

import stdx.net.http.*
import std.time.*
import std.collection.*

var time1 = DateTime.ofUTC(year: 2022, month: January, dayOfMonth: 19)
var cookie = Cookie("0123456789", "789", path: "/a1/", domain: ".examplenet.com", expires: time1, maxAge: 1, secure: true,
    httpOnly: true)
var cookie1 = Cookie("ABCDEFGHIJKLMNOPQRSTUVWXYZ", "", path: "/a1/", domain: ".examplenet.com", expires: time1, maxAge: 1,
    secure: true, httpOnly: true)
var cookie2 = Cookie("abcdefghijklmnopqrstuvwxyz", "9", path: "/a1/", domain: ".examplenet.com", expires: time1,
    maxAge: 1, secure: true, httpOnly: true)
var cookie3 = Cookie("!#$%&*+-.^_`|~", "", path: "/a1/", domain: ".examplenet.com", expires: time1, maxAge: 1,
    secure: true, httpOnly: true)

main(): Int64 {
    var a = cookie.toSetCookieString()
    var a1 = cookie1.toSetCookieString()
    var a2 = cookie2.toSetCookieString()
    var a3 = cookie3.toSetCookieString()
    if (a.contains("0123456789=789; Expires=Wed, 19 Jan 2022 00:00:00 UTC;") != true) {
        return 1
    }
    if (a.contains("Max-Age=1; Domain=examplenet.com; Path=/a1/; Secure; HttpOnly") != true) {
        return 2
    }
    if (a1.contains("ABCDEFGHIJKLMNOPQRSTUVWXYZ=; Expires=Wed, 19 Jan 2022 00:00:00 UTC;") != true) {
        return 3
    }
    if (a1.contains("Max-Age=1; Domain=examplenet.com; Path=/a1/; Secure; HttpOnly") != true) {
        return 4
    }
    if (a2.contains("abcdefghijklmnopqrstuvwxyz=9; Expires=Wed, 19 Jan 2022 00:00:00 UTC;") != true) {
        return 5
    }
    if (a2.contains("Max-Age=1; Domain=examplenet.com; Path=/a1/; Secure; HttpOnly") != true) {
        return 6
    }
    if (a3.contains("!#$%&*+-.^_`|~=; Expires=Wed, 19 Jan 2022 00:00:00 UTC; Max-Age=1;") != true) {
        return 7
    }
    if (a3.contains("Domain=examplenet.com; Path=/a1/; Secure; HttpOnly") != true) {
        return 8
    }
    return 0
}
