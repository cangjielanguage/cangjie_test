// LEVEL: 2
// EXEC: %compiler %cmp_opt -o %n.%suffix %f %cmp_utest_opt
// RUN-EXEC: %run %run_opt %n.%suffix %run_utest_opt %run_args

import std.collection.*

@Test
class Test_all_15 {
    /*
     * Test description:
     *     test the func of all set the Int
     * Test API:
     *     public func all(predicate: (T)-> Bool): Bool { ... }
     */
    @TestCase
    func test_all_01(): Unit {
        var emptyArr1: Array<Int64> = Array<Int64>()
        var normalArr1: Array<Int64> = Array<Int64>(10, repeat: 1)
        @Expect(emptyArr1.iterator().all({val: Int64 => (val * 2 == 0)}))
        @Expect(!normalArr1.iterator().all({val: Int64 => (val % 2 == 0)}))
    }

    /*
     * Test description:
     *     test the func of all set the Int
     * Test API:
     *     public func all(predicate: (T)-> Bool): Bool { ... }
     */
    @TestCase
    func test_all_02(): Unit {
        var emptyArr1: Array<String> = Array<String>()
        var normalArr1: Array<String> = Array<String>(10, repeat: "2")
        @Expect(emptyArr1.iterator().all({val: String => val != "2"}))
        @Expect(emptyArr1.iterator().all({val: String => val == "2"}))
        @Expect(normalArr1.iterator().all({val: String =>val == "2"}))
        @Expect(!normalArr1.iterator().all({val: String =>val != "2"}))
    }

    /*
     * Test description:
     *     test the func of all set the Float64
     * Test API:
     *     public func all(predicate: (T)-> Bool): Bool { ... }
     */
    @TestCase
    func test_all_03(): Unit {
        var emptyArr1: Array<Float64> = Array<Float64>()
        var normalArr1: Array<Float64> = Array<Float64>(10, repeat: Float64(1.11111))
        @Expect(emptyArr1.iterator().all({val: Float64 => val != Float64(1.11111)}))
        @Expect(emptyArr1.iterator().all({val: Float64 => val == Float64(1.11111)}))
        @Expect(normalArr1.iterator().all({val: Float64 =>val == Float64(1.11111)}))
        @Expect(!normalArr1.iterator().all({val: Float64 =>val != Float64(1.11111)}))
    }

    /*
     * Test description:
     *     test the func of all set the Rune
     * Test API:
     *     public func all(predicate: (T)-> Bool): Bool { ... }
     */
    @TestCase
    func test_all_04(): Unit {
        var emptyArr1: Array<Rune> = Array<Rune>()
        var normalArr1: Array<Rune> = Array<Rune>(10, repeat: r'♬')
        @Expect(emptyArr1.iterator().all({val: Rune => val != r'♬'}))
        @Expect(emptyArr1.iterator().all({val: Rune => val == r'♬'}))
        @Expect(normalArr1.iterator().all({val: Rune =>val == r'♬'}))
        @Expect(!normalArr1.iterator().all({val: Rune =>val != r'♬'}))
    }
}
