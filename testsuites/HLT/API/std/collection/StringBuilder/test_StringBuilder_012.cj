import std.math.*

main(): Int32 {
    var strb4 = StringBuilder("123")
    if (strb4.toString() != "123") {
        return 4
    }

    var strb5 = StringBuilder([r'A', r'B', r'C', r'D', r'E', r'F'])
    if (strb5.toString() != "ABCDEF") {
        return 5
    }

    var arr6: Array<Rune> = Array<Rune>(1024, repeat: r'A')
    var strb6 = StringBuilder(arr6)
    if (strb6.toString() !=
        "") {
        return 6
    }

    var strb7 = StringBuilder([r'b', r' ', r'c', r'戸', r'E', r'@', r'。', r'a'])
    if (strb7.toString() != "b c戸E@。a") {
        return 7
    }

    return 0
}
