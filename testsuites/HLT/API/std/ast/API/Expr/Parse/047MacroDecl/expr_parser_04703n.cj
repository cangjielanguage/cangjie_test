// LEVEL: 1
// EXEC: %compiler %cmp_opt %f -o %output
// RUN-ERRCHECK: %run %run_opt %output %run_args
// ASSERT: scan Not MacroDecl

import std.ast.*

main() {
    var macroTokens = quote(public func X (input:Tokens) : Tokens { return input })
    let expr = parseDecl(macroTokens)
    var macroDecl = match (expr as MacroDecl) {
        case Some(v) => v
        case None => throw Exception("Not MacroDecl")
    }
    return 0
}
