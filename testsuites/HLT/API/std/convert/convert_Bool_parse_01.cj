import std.convert.*
import std.collection.*

main(): Int64 {
    var arr1 = ["true", "false", #"true"#, #"false"#, "${true}"]
    var arr2 = ArrayList<Bool>()
    var arr3 = ArrayList<Bool>([true, false, true, false, true])
    for (i in arr1) {
        arr2.add(Bool.parse(i))
    }
    if (arr2 != arr3) {
        return 1
    }
    return 0
}
