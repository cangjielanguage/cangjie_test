// RUN-ERRCHECK: %run %run_opt %output %run_args
// ASSERT: scan NoneValueException

import std.regex.*

main(): Int64 {
    var pattern = "\\0"
    var str = "\\0"
    var regex = Regex(pattern)
    var matcher = Matcher(regex, str)
    var res = matcher.findAll().getOrThrow()
    for (i in res) {
        println("res is ${i.matchString()}")
    }
    return 0
}
