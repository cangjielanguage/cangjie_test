// LEVEL: 1
// EXEC: %compiler %cmp_opt -o %n.%suffix %f %cmp_utest_opt
// RUN-EXEC: %run %run_opt %n.%suffix %run_utest_opt %run_args

import std.math.numeric.*
import std.math.*
import std.collection.*

var sum = 0

@Test
class Test_decimal_typeconvert_string_36 {
    public override func beforeEach(): Unit {
        sum = 0
    }
    @TestCase
    func test_decimal_typeconvert_string_01(): Unit {
        let dec1 = Decimal.parse("10e-5")
        @Expect(dec1.toFloat16(), Float16(0.0001))
        @Expect(dec1.toFloat32(), Float32(0.0001))
        @Expect(dec1.toFloat64(), Float64(0.0001))
        @Expect(dec1.toBigInt(), BigInt(0))
        @Expect(dec1.toInt8(), 0)
        @Expect(dec1.toInt16(), 0)
        @Expect(dec1.toInt32(), 0)
        @Expect(dec1.toInt64(), 0)
        @Expect(dec1.toIntNative(), 0)
        @Expect(dec1.toUInt8(), 0)
        @Expect(dec1.toUInt16(), 0)
        @Expect(dec1.toUInt32(), 0)
        @Expect(dec1.toUInt64(), 0)
        @Expect(dec1.toUIntNative(), 0)

        @Expect(dec1.toInt8(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toInt16(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toInt32(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toInt64(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toIntNative(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toUInt8(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toUInt16(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toUInt32(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toUInt64(overflowHandling:OverflowStrategy.Throwing), 0)
        @Expect(dec1.toUIntNative(overflowHandling:OverflowStrategy.Throwing), 0)

        @Expect(dec1.toInt8(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toInt16(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toInt32(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toInt64(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toIntNative(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toUInt8(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toUInt16(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toUInt32(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toUInt64(overflowHandling:OverflowStrategy.Wrapping), 0)
        @Expect(dec1.toUIntNative(overflowHandling:OverflowStrategy.Wrapping), 0)

        @Expect(dec1.toInt8(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toInt16(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toInt32(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toInt64(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toIntNative(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toUInt8(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toUInt16(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toUInt32(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toUInt64(overflowHandling:OverflowStrategy.Saturating), 0)
        @Expect(dec1.toUIntNative(overflowHandling:OverflowStrategy.Saturating), 0)
    }
}
