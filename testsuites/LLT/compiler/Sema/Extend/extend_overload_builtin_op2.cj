interface Eq<T> {
    operator func ==(right: T): Bool
}

extend Int64 <: Eq<Int64> {} // OK, ==(right: Int64) will be generated by the compiler

extend Int64 <: Eq<Int32> {}

extend Int64 <: Eq<Int16> {
    public operator func ==(right: Int16): Bool { // OK
        return this == Int64(right)
    }
}

