package arithmetic

public class Vector2d {
    public let x: Int64
    public let y: Int64

    public init(x: Int64, y: Int64) {
        this.x = x
        this.y = y
    }
}

public func add(x: Int64, y: Int64): Int64 {
    return x + y
}
