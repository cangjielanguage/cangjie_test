// LEVEL: 0
// EXEC: %compiler %f -o %n.%suffix
// EXEC-1: %run %run_opt %n.%suffix

open class A {
    static func goo() {
        2
    }
}

class B <: A {
    static func goo() {
        1
    }
}

func foo<T>() where T <: A {
    T.goo()
}

main() {
    foo<A>() - foo<B>()
}
