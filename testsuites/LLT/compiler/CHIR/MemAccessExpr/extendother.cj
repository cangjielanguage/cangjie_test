// LEVEL: 0
// EXEC: %compiler %cmp_opt %n.cj -o %output
// EXEC: %run %run_opt %output %run_args

public open class Foo {}

extend Foo {
    func f1() {
        return 1
    }
    static func f2() {
        return 2
    }
}

main(): Int64 {
    var a = Foo().f1
    var b = Foo.f2
    if (a() != 1 || b() != 2) {
        return 1
    }
    return 0
}
