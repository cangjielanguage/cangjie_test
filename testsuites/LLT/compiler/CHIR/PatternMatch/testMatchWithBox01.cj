// LEVEL: 0
// EXEC:%compiler %n.cj -o %output
// EXEC: %run %output
// ASSERT: scan-not core dumped
// ASSERT: scan-not stack trace

interface I {
    func f(): Int64
}

class Foo {
    func g() {}
}

extend Foo <: I {
    public func f() {
        11
    }
}

main() {
    var i = (10, Foo())
    var res = match (i) {
        case (10, d: I) => d.f()
        case _ => 100
    }

    if (res == 11) {
        return 0
    } else {
        return 1
    }
}
