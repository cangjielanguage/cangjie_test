// EXEC: %compiler %cmp_opt %n.cj -o %output
// EXEC: %run %run_opt %output %run_args

open class Foo1 {
    var dt1: Int32 = 2
}

class Bar1 <: Foo1 {
    var dt: Int32 = 4
}

enum Time<T1,T2>
    where T1 <: Foo1{
    Day(T1) | Hour(T2)
}

func run(): Int64 {
    var bar1: Bar1= Bar1()
    var time1  = Time<Bar1, Int32>.Day(bar1)
    var time2  = Time<Bar1, Int32>.Hour(6)
    var res1: Int32 = match (time1) {
        case Day(x) => x.dt
        case Hour(x) => x
    }
    var res2: Int32 = match (time2) {
        case Day(x) => x.dt
        case Hour(x) => x
    }
    if (res1 == 4 && res2 == 6) {
        return 0
    }
    return 1
}

func main(): Int64 {
    return run()
}
