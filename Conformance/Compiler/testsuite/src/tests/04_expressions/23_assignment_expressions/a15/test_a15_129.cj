/*
  @Name:        04_23_a15_129

  @Level:         1

  @Assertion:   4.23(15) A left value expression is legal only if it is mutable. For the mutability of the above
                expressions, see the corresponding sections.
  @Description: Check that immutable optional chaining expression being used on left-hand side of assignment
                expression (^=) causes a compilation error. Check case 'a?.b.c?.d = e1'.
  @Mode: compileonly
  @Negative: yes
  @Structure: single
  @CompileWarning: ignore
  @Comment: Auto-generated by gen.py

*/

struct C {
    let d = 1
}

struct B {
    var c = Option<C>.Some(C())
}

struct A {
    var b = B()
}

main() {
    var a = Option<A>.Some(A())
    a?.b.c?.d ^= 1
    return 0
}
