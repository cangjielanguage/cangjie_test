interface I {}

interface J <: I {}

interface K <: J {
    operator func <<(x : Unit) : CJ
}

interface L <: I {}

open class CI <: I {
    public operator func <<(x : Unit) { CJ() }
}

open class CJ <: J {
    public operator func <<(x : Unit) { CJ() }
}

open class CK <: K {
    public operator func <<(x : Unit) { CJ() }
    static func k() : K { CK() }
}

open class CL <: L {
    public operator func <<(x : Unit) { CJ() }
