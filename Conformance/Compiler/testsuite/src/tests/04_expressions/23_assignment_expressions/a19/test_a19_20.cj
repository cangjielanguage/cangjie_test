/*
  @Name:        04_23_a19_20

  @Level:         1

  @Assertion:   4.23(19) Note that = is non-associative, which implies that expression like a = b = 0 that contains
                more than one = is a syntactic error.
  @Description: Checks that a = b = c is not permitted for type C.
  @Mode: compileonly
  @Negative: yes
  @Structure: single
  @CompileWarning: ignore
  @Comment: Auto-generated by gen.py

*/

class C {
    let x : Int64
    C(y : Int64) {
        x = y
    }
}

main() {
    var v1 : C = C(1)
    var v2 : C = C(1)
    v1 = v2 = C(1)
}
