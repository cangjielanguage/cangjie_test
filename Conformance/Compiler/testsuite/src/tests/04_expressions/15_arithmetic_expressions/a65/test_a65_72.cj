/*
  @Name:        04_15_a65_72

  @Level:         1

  @Assertion:   4.15(65) The default strategy is the same with @OverflowThrowing.
  @Description: Checks for UInt64 a, b that a * b causes an ArithmeticException (upper bound overflow) if
                @OverflowThrowing is used.
  @Mode: run
  @Negative: no
  @Structure: single
  @CompileWarning: no
  @Comment: Auto-generated by gen_exception.py
*/

import utils.assert.Assert

@OverflowThrowing
func f(a: UInt64, b: UInt64) {
    a * b
}

main() {
    let a = 18446744073709551615u64
    let b = 18446744073709551615u64
    var caught : Bool = false
    try {
        f(a, b)
    } catch (e: ArithmeticException) {
        caught = true
    }
    Assert.isTrue(caught)
}
