/*
  @Name:        04_16_a01_01

  @Level:         1

  @Assertion:   4.16(1) Relational expressions are expressions that contain relational operators. Threre are six kinds
  of relational operators, i.e., equal (==), not equal (!=), less than (<), less than or equal (<=), greater than (>),
  and greater than or equal (>=).
  @Description: Checks compilation and execution of a program that uses relational operators. 
  @Mode: run
  @Negative: no
  @Structure: single
  @CompileWarning: no

*/

import utils.assert.Assert

main() {
    let x = 1
    let y = 2
    let fx = 1.0
    let fy = 2.0

    Assert.isFalse(1==2)
    Assert.isFalse(x==2)
    Assert.isFalse((x)==2)
    Assert.isFalse((x)==2)
    Assert.isFalse(1==y)
    Assert.isFalse(x==y)
    Assert.isFalse((x)==y)
    Assert.isFalse((x)==y)
    Assert.isFalse(1==(y))
    Assert.isFalse(x==(y))
    Assert.isFalse((x)==(y))
    Assert.isFalse((x)==(y))
    Assert.isFalse(1==(y))
    Assert.isFalse(x==(y))
    Assert.isFalse((x)==(y))
    Assert.isFalse((x)==(y))

    Assert.isFalse(1.0==2.0)
    Assert.isFalse(fx==2.0)
    Assert.isFalse((fx)==2.0)
    Assert.isFalse((fx)==2.0)
    Assert.isFalse(1.0==fy)
    Assert.isFalse(fx==fy)
    Assert.isFalse((fx)==fy)
    Assert.isFalse((fx)==fy)
    Assert.isFalse(1.0==(fy))
    Assert.isFalse(fx==(fy))
    Assert.isFalse((fx)==(fy))
    Assert.isFalse((fx)==(fy))
    Assert.isFalse(1.0==(fy))
    Assert.isFalse(fx==(fy))
    Assert.isFalse((fx)==(fy))
    Assert.isFalse((fx)==(fy))

    Assert.isTrue(1!=2)
    Assert.isTrue(x!=2)
    Assert.isTrue((x)!=2)
    Assert.isTrue((x)!=2)
    Assert.isTrue(1!=y)
    Assert.isTrue(x!=y)
    Assert.isTrue((x)!=y)
    Assert.isTrue((x)!=y)
    Assert.isTrue(1!=(y))
    Assert.isTrue(x!=(y))
    Assert.isTrue((x)!=(y))
    Assert.isTrue((x)!=(y))
    Assert.isTrue(1!=(y))
    Assert.isTrue(x!=(y))
    Assert.isTrue((x)!=(y))
    Assert.isTrue((x)!=(y))

    Assert.isTrue(1.0!=2.0)
    Assert.isTrue(fx!=2.0)
    Assert.isTrue((fx)!=2.0)
    Assert.isTrue((fx)!=2.0)
    Assert.isTrue(1.0!=fy)
    Assert.isTrue(fx!=fy)
    Assert.isTrue((fx)!=fy)
    Assert.isTrue((fx)!=fy)
    Assert.isTrue(1.0!=(fy))
    Assert.isTrue(fx!=(fy))
    Assert.isTrue((fx)!=(fy))
    Assert.isTrue((fx)!=(fy))
    Assert.isTrue(1.0!=(fy))
    Assert.isTrue(fx!=(fy))
    Assert.isTrue((fx)!=(fy))
    Assert.isTrue((fx)!=(fy))

    Assert.isTrue(1<=2)
    Assert.isTrue(x<=2)
    Assert.isTrue((x)<=2)
    Assert.isTrue((x)<=2)
    Assert.isTrue(1<=y)
    Assert.isTrue(x<=y)
    Assert.isTrue((x)<=y)
    Assert.isTrue((x)<=y)
    Assert.isTrue(1<=(y))
    Assert.isTrue(x<=(y))
    Assert.isTrue((x)<=(y))
    Assert.isTrue((x)<=(y))
    Assert.isTrue(1<=(y))
    Assert.isTrue(x<=(y))
    Assert.isTrue((x)<=(y))
    Assert.isTrue((x)<=(y))

    Assert.isTrue(1.0<=2.0)
    Assert.isTrue(fx<=2.0)
    Assert.isTrue((fx)<=2.0)
    Assert.isTrue((fx)<=2.0)
    Assert.isTrue(1.0<=fy)
    Assert.isTrue(fx<=fy)
    Assert.isTrue((fx)<=fy)
    Assert.isTrue((fx)<=fy)
    Assert.isTrue(1.0<=(fy))
    Assert.isTrue(fx<=(fy))
    Assert.isTrue((fx)<=(fy))
    Assert.isTrue((fx)<=(fy))
    Assert.isTrue(1.0<=(fy))
    Assert.isTrue(fx<=(fy))
    Assert.isTrue((fx)<=(fy))
    Assert.isTrue((fx)<=(fy))

    Assert.isTrue(1<2)
    Assert.isTrue(x<2)
    Assert.isTrue((x)<2)
    Assert.isTrue((x)<2)
    Assert.isTrue(1<y)
    Assert.isTrue(x<y)
    Assert.isTrue((x)<y)
    Assert.isTrue((x)<y)
    Assert.isTrue(1<(y))
    Assert.isTrue(x<(y))
    Assert.isTrue((x)<(y))
    Assert.isTrue((x)<(y))
    Assert.isTrue(1<(y))
    Assert.isTrue(x<(y))
    Assert.isTrue((x)<(y))
    Assert.isTrue((x)<(y))

    Assert.isTrue(1.0<2.0)
    Assert.isTrue(fx<2.0)
    Assert.isTrue((fx)<2.0)
    Assert.isTrue((fx)<2.0)
    Assert.isTrue(1.0<fy)
    Assert.isTrue(fx<fy)
    Assert.isTrue((fx)<fy)
    Assert.isTrue((fx)<fy)
    Assert.isTrue(1.0<(fy))
    Assert.isTrue(fx<(fy))
    Assert.isTrue((fx)<(fy))
    Assert.isTrue((fx)<(fy))
    Assert.isTrue(1.0<(fy))
    Assert.isTrue(fx<(fy))
    Assert.isTrue((fx)<(fy))
    Assert.isTrue((fx)<(fy))

    Assert.isFalse(1>2)
    Assert.isFalse(x>2)
    Assert.isFalse((x)>2)
    Assert.isFalse((x)>2)
    Assert.isFalse(1>y)
    Assert.isFalse(x>y)
    Assert.isFalse((x)>y)
    Assert.isFalse((x)>y)
    Assert.isFalse(1>(y))
    Assert.isFalse(x>(y))
    Assert.isFalse((x)>(y))
    Assert.isFalse((x)>(y))
    Assert.isFalse(1>(y))
    Assert.isFalse(x>(y))
    Assert.isFalse((x)>(y))
    Assert.isFalse((x)>(y))

    Assert.isFalse(1.0>2.0)
    Assert.isFalse(fx>2.0)
    Assert.isFalse((fx)>2.0)
    Assert.isFalse((fx)>2.0)
    Assert.isFalse(1.0>fy)
    Assert.isFalse(fx>fy)
    Assert.isFalse((fx)>fy)
    Assert.isFalse((fx)>fy)
    Assert.isFalse(1.0>(fy))
    Assert.isFalse(fx>(fy))
    Assert.isFalse((fx)>(fy))
    Assert.isFalse((fx)>(fy))
    Assert.isFalse(1.0>(fy))
    Assert.isFalse(fx>(fy))
    Assert.isFalse((fx)>(fy))
    Assert.isFalse((fx)>(fy))

    Assert.isFalse(1>=2)
    Assert.isFalse(x>=2)
    Assert.isFalse((x)>=2)
    Assert.isFalse((x)>=2)
    Assert.isFalse(1>=y)
    Assert.isFalse(x>=y)
    Assert.isFalse((x)>=y)
    Assert.isFalse((x)>=y)
    Assert.isFalse(1>=(y))
    Assert.isFalse(x>=(y))
    Assert.isFalse((x)>=(y))
    Assert.isFalse((x)>=(y))
    Assert.isFalse(1>=(y))
    Assert.isFalse(x>=(y))
    Assert.isFalse((x)>=(y))
    Assert.isFalse((x)>=(y))

    Assert.isFalse(1.0>=2.0)
    Assert.isFalse(fx>=2.0)
    Assert.isFalse((fx)>=2.0)
    Assert.isFalse((fx)>=2.0)
    Assert.isFalse(1.0>=fy)
    Assert.isFalse(fx>=fy)
    Assert.isFalse((fx)>=fy)
    Assert.isFalse((fx)>=fy)
    Assert.isFalse(1.0>=(fy))
    Assert.isFalse(fx>=(fy))
    Assert.isFalse((fx)>=(fy))
    Assert.isFalse((fx)>=(fy))
    Assert.isFalse(1.0>=(fy))
    Assert.isFalse(fx>=(fy))
    Assert.isFalse((fx)>=(fy))
    Assert.isFalse((fx)>=(fy))

    Assert.isTrue(true == true)
    Assert.isFalse(true != true)
    Assert.isTrue((1, 1) == (1, 1))
    Assert.isFalse((1, 1) != (1, 1))
    Assert.isTrue(() == ())
    Assert.isFalse(() != ())
    Assert.isTrue([1] == [1])
    Assert.isFalse([1] != [1])

    Assert.isTrue('1' == '1')
    Assert.isFalse('1' != '1')
    Assert.isFalse('1' > '1')
    Assert.isFalse('1' < '1')
    Assert.isTrue('1' >= '1')
    Assert.isTrue('1' <= '1')

    Assert.isTrue("1" == "1")
    Assert.isFalse("1" != "1")
    Assert.isFalse("1" > "1")
    Assert.isFalse("1" < "1")
    Assert.isTrue("1" >= "1")
    Assert.isTrue("1" <= "1")

    0
}
